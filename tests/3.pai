# Print the values of an array in increasing order

arr = [-77, 100, 50, 50, 50, 1, 2, 7, 3, 5, 1];
n = len $ arr;

not_processed = [];
i = 0;
while i < n {
    not_processed = not_processed + [1];
    i = i + 1;
}

times = 0;
while times < n {
    min = 99999999;
    min_idx = -1;

    i = 0;
    while i < n {
        if not_processed[i] && min > arr[i] {
            min = arr[i];
            min_idx = i;
        }

        i = i + 1;
    }

    min; # Print the current minimum

    # Add min_idx to the list of processed indexes
    tmp = [];
    i = 0;
    while i < n {
        if i == min_idx || !not_processed[i] {
            tmp = tmp + [0];
        } else {
            tmp = tmp + [1];
        }

        i = i + 1;
    }

    # Overwrite the old list 
    not_processed = tmp;

    times = times + 1;
}
