# Read numbers from standard input and calculate their product

"Input n:";
n = read $ _;

if n < 1 {
    "Bad input";
} else {
    "Got n:";
    n;

    i = 0;
    prod = 1;
    while i < n {
        "Input number";
        x = read $ _;

        prod = prod * x;
        i = i + 1;
    }

    prod; # Print the product
}
